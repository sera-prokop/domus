            }

footer.footer
  section.footer-top

    .footer-top__left
      .footer-top__logo
        a(href="/")
          img(src="img/logo.svg", alt="Logo").footer-top__img
      p.footer-top__left-address Кушелевская дорога, д3, к.2
      p.footer-top__left-tel
        a(href="tel:+78129057471").footer-top__left-tel-link +7 (812) 905-74-71

    .footer-top__center.footer-map
      ul.footer-map__list
        each value, key in map
          li.footer-map__item
            a(href="javascript:;").footer-map__link= key
            if(key==="Услуги")
              ul.map-submenu__list
                each value, key in services
                  li.map-submenu__item
                    a(href="javascript:;").map-submenu__link= key
            if(key==="Магазин")
              ul.map-submenu__list
                each value, key in shop
                  li.map-submenu__item
                    a(href="javascript:;").map-submenu__link= key
            if(key==="Новости")
              ul.map-submenu__list
                each value, key in news
                  li.map-submenu__item
                    a(href="javascript:;").map-submenu__link= key


    .footer-top__right.footer-social
      h3.footer-social__title Мы в соцсетях:
